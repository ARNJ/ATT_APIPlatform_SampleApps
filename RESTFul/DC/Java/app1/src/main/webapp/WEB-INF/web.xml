<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>location</display-name>
  <welcome-file-list>
    <welcome-file>DC.jsp</welcome-file>
  </welcome-file-list>
  <display-name>AT&amp;T DC Sample Application</display-name>

  <servlet>
    <servlet-name>DCController</servlet-name>
    <servlet-class>com.att.api.dc.controller.DCController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoadController</servlet-name>
    <servlet-class>com.att.api.dc.controller.LoadController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SaveController</servlet-name>
    <servlet-class>com.att.api.dc.controller.SaveController</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>OAuthController</servlet-name>
      <servlet-class>com.att.api.dc.controller.OAuthController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetCapabilitiesController</servlet-name>
    <servlet-class>com.att.api.dc.controller.GetCapabilitiesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DCController</servlet-name>
    <url-pattern>/index.jsp</url-pattern> 
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DCController</servlet-name>
    <url-pattern>/DC.jsp</url-pattern> 
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DCController</servlet-name>
    <url-pattern>/dc.jsp</url-pattern> 
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>OAuthController</servlet-name>
      <url-pattern>/oauth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>LoadController</servlet-name>
      <url-pattern>/load</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>SaveController</servlet-name>
      <url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>GetCapabilitiesController</servlet-name>
      <url-pattern>/getDeviceCapabilities</url-pattern>
  </servlet-mapping>
</web-app>
